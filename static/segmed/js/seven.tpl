        row["Gravado"] = 0;

        ///calculo de SDI
        var salarioDiario = row["ManutencionParaCalculoISRAsimilableASalarios"]/30.4; //magic number
        var minLey = diasDeVacaciones * salarioDiario * 0.25;
        var primaDiaria = minLey / 365;
